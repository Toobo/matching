{
  "name":         "toobo/matching",
  "description":  "Kind of pattern matching for PHP",
  "keywords":     [
    "pattern matching",
    "functional programming"
  ],
  "authors":      [
    {
      "name":     "Giuseppe Mazzapica",
      "email":    "giuseppe.mazzapica@gmail.com",
      "homepage": "https://gmazzap.me",
      "role":     "Developer"
    }
  ],
  "license":      "MIT",
  "require":      {
    "php": ">=7"
  },
  "require-dev":  {
    "phpunit/phpunit": "^6.2"
  },
  "autoload":     {
    "psr-4": {
      "Toobo\\Matching\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Toobo\\Matching\\Tests\\": "tests/"
    }
  },
  "config":       {
    "optimize-autoloader": true
  },
  "extra":        {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
